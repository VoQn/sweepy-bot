{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "eofline": false,
        "indent": [
            true,
            "spaces",
            4
        ],
        "object-literal-sort-keys": false,
        "quotemark": [
            true,
            "single"
        ],
        "trailing-comma": false,
        "semicolon": [
            true,
            "never"
        ],
        "max-line-length": false,
        "no-var-requires": false,
        "no-empty-interface": false,
        "no-empty": false,
        "no-console": false,
        "max-classes-per-file": false,
        "variable-name": [
            true,
            "ban-keywords",
            "check-format",
            "allow-leading-underscore"
        ]
    },
    "rulesDirectory": []
}