{
  "name": "sweepy-bot",
  "version": "1.0.0",
  "description": "A simple Discord chat bot about Oxygen Not Included",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rm -rf ./dist",
    "compile": "tsc",
    "test:run": "jest",
    "start:server": "node ./dist/server.js",
    "build": "run-s clean compile",
    "test": "run-s clean compile test:run",
    "start": "run-s build start:server"
  },
  "dependencies": {
    "discord.js": "^12.3.1",
    "express": "^4.17.1",
    "global": "^4.4.0",
    "querystring": "^0.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.7",
    "@types/node": "^14.6.0",
    "jest": "^26.4.2",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^26.2.0",
    "tslint": "^6.1.3",
    "typescript": "^3.9.0"
  },
  "engines": {
    "node": "12.x"
  },
  "repository": {
    "url": "https://glitch.com/edit/#!/sweepy-bot"
  },
  "license": "MIT",
  "keywords": [
    "node",
    "glitch",
    "express"
  ],
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/src/**/*.test.ts"
    ]
  }
}
