{
  "name": "sweepy-bot",
  "version": "1.0.0",
  "description": "A simple Discord chat bot about Oxygen Not Included",
  "main": "./dist/src/index.js",
  "scripts": {
    "clean": "rm -rf ./dist",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --fix --ext .js,.ts",
    "build": "tsc",
    "test": "jest",
    "start": "ts-node src/index.ts",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.(j|t)sx?": [
      "pnpm lint:fix",
      "git add"
    ]
  },
  "dependencies": {
    "@discordjs/collection": "^0.1.6",
    "discord.js": "^12.3.1",
    "express": "^4.17.1",
    "global": "^4.4.0",
    "querystring": "^0.2.0",
    "ts-node": "^9.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.7",
    "@types/jest": "^26.0.9",
    "@types/node": "^14.6.0",
    "@types/ws": "^7.2.6",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "jest": "^26.4.2",
    "npm-run-all": "^4.1.5",
    "prettier": "2.1.1",
    "ts-jest": "^26.2.0",
    "typescript": "^3.9.0"
  },
  "engines": {
    "node": "12.x",
    "pnpm": "5.5.x"
  },
  "repository": {
    "url": "https://glitch.com/edit/#!/sweepy-bot"
  },
  "license": "MIT",
  "keywords": [
    "node",
    "glitch",
    "express"
  ]
}
