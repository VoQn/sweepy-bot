{
  "private": true,
  "name": "sweepy-bot",
  "version": "0.0.1",
  "description": "A simple Discord chat bot about Oxygen Not Included",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/VoQn/sweepy-bot.git"
  },
  "scripts": {
    "clean": "pnpm recursive run clean --no-bail",
    "lint": "pnpm recursive run lint --no-bail",
    "lint:fix": "pnpm recursive run lint:fix",
    "format": "pnpm recursive run format --no-bail",
    "typecheck": "pnpm recursive run typecheck",
    "build": "pnpm recursive run build",
    "test": "pnpm recursive test",
    "start": "pnpm start --filter app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pnpm lint:fix && pretty-quick --staged",
      "pre-push": "pnpm test"
    }
  },
  "devDependencies": {
    "husky": "^4.2.5",
    "jest": "^26.4.2",
    "lint-staged": "^10.2.13",
    "prettier": "^2.1.1",
    "pretty-quick": "^3.0.0"
  },
  "engines": {
    "node": "12.x",
    "pnpm": "5.5.x"
  },
  "keywords": [
    "node",
    "glitch",
    "express"
  ]
}
