    "@sweepy-bot/prettier": ">=0",
    "@types/express": "^4.17.7",
    "@types/jest": "^26.0.9",
    "@types/node": "^14.6.0",
    "@types/ws": "^7.2.6",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "typescript": "^4.0.2"
