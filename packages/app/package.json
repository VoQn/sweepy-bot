{
  "private": true,
  "name": "@sweepy-bot/app",
  "version": "0.0.1",
  "description": "A simple Discord chat bot about Oxygen Not Included",
  "author": "Kazuhiro Mizushima <voqn.tyrant@gmail.com>",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/VoQn/sweepy-bot.git"
  },
  "main": "./dist/src/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit --",
    "lint": "eslint src --ext .js,.ts",
    "lint:fix": "eslint src --fix --ext .js,.ts",
    "format": "prettier -w src",
    "build": "babel src -x .ts,.tsx -d dist",
    "test": "NODE_ENV=test jest",
    "start": "ts-node src/index.ts"
  },
  "dependencies": {
    "@discordjs/collection": "^0.1.6",
    "discord.js": "^12.3.1",
    "express": "^4.17.1",
    "global": "^4.4.0",
    "querystring": "^0.2.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-transform-modules-commonjs": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@sweepy-bot/eslint-config": "workspace:^0.0.1",
    "@sweepy-bot/prettier": "workspace:^0.0.1",
    "@types/express": "^4.17.7",
    "@types/jest": "^26.0.9",
    "@types/node": "^14.6.0",
    "@types/ws": "^7.2.6",
    "jest": "^26.4.2",
    "npm-run-all": "^4.1.5",
    "prettier": "2.1.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.2.0"
  },
  "keywords": [
    "node",
    "glitch",
    "express"
  ],
  "prettier": "@sweepy-bot/prettier"
}
